<%= simple_form_for(@founder, url: founders_profile_path) do |f| %>
 <%= f.error_notification %>

 <%= f.input :first_name %>
 <%= f.input :last_name %>
 <%= f.input :website %>
 <%= f.input :profile_pic %>
 <%= f.input :facebook %>
 <%= f.input :twitter %>
 <%= f.input :linkedin %>
 <%= f.input :angellist %>
 <%= f.input :skype %>
 <%= f.input :cell_phone %>

 <%= f.input :all_startups %>
 <%= f.button :submit, "Save", class: "btn btn-primary" %>
<% end %>